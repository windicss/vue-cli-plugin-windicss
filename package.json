{
  "name": "vue-cli-plugin-windicss",
  "version": "1.0.3",
  "description": "Windi CSS for Vue CLI",
  "main": "index.js",
  "types": "dist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windicss/vue-cli-plugin-windicss.git"
  },
  "files": [
    "/dist",
    "/README.md",
    "/index.js",
    "/package.json",
    "/generator"
  ],
  "keywords": [
    "vue",
    "cli",
    "windi",
    "windicss"
  ],
  "scripts": {
    "dev:vue2": "yarn build && cd examples/vue2 && yarn serve",
    "dev:vue3": "yarn build && cd examples/vue3 && yarn serve",
    "build": "yarn clean && bili src/index.ts",
    "clean": "rimraf dist",
    "test": "true",
    "release": "dotenv release-it --",
    "lint": "eslint --fix --ext .ts,.js,.vue ."
  },
  "author": "Harlan Wilton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windicss/vue-cli-plugin-windicss/issues"
  },
  "homepage": "https://github.com/windicss/vue-cli-plugin-windicss#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@vue/cli-service": "^3.0.0 || ^4.0.0-0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.4",
    "@types/debug": "^4.1.5",
    "@types/loader-utils": "^2.0.2",
    "@types/lodash": "^4.14.170",
    "@types/node": "15.6.0",
    "@types/webpack": "^5.28.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/cli-shared-utils": "^4.5.13",
    "@windicss/plugin-utils": "^1.0.1",
    "bili": "^5.0.5",
    "chalk": "^4.1.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.29.0",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "pug": "^3.0.2",
    "release-it": "^14.6.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.2.4",
    "upath": "^2.0.1"
  },
  "dependencies": {
    "defu": "^4.0.1",
    "windicss-webpack-plugin": "^1.4.6"
  }
}
